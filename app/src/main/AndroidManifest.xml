<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.FarsangApp"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LeirasActivity"></activity>
        <activity android:name=".FirstQuestion"></activity>
        <activity android:name=".LeirasKettoActivity"></activity>
        <activity android:name=".SecondQuestion"></activity>
        <activity android:name=".ThirdQuestion"></activity>
        <activity android:name=".LeirasThird"></activity>
        <activity android:name=".FourthQuestion"></activity>
        <activity android:name=".FifthQuestion"></activity>
        <activity android:name=".FinalPage"></activity>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>